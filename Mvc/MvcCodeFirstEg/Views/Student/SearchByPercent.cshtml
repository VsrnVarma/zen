
@{
    ViewBag.Title = "SearchByPercent";
}

<h2>Search Students By Percent</h2>

@using (Html.BeginForm("StudPercent", "Student", FormMethod.Get))
{
    <hr />
    <div class="form-horizontal">
        @Html.DisplayName("Start")
        @Html.TextBox("start")
        @Html.DisplayName("End")
